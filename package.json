{
  "name": "tilmeld-client",
  "version": "1.0.0-beta.22",
  "description": "Nymph user and group management with access controls.",
  "main": "lib/umd/TilmeldClient.js",
  "browser": "lib/es/TilmeldClient.js",
  "directories": {
    "lib": "lib/umd"
  },
  "scripts": {
    "build-components": "svelte compile src/Components -o lib/es/Components",
    "build-entities": "cp -a src/Entities lib/es/",
    "build-index": "cp src/TilmeldClient.js lib/es/",
    "transpile": "babel -d lib/umd/ lib/es/",
    "build": "npm run build-components && npm run build-entities && npm run build-index && npm run transpile",
    "prepare": "npm run lint && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/Components/*.html src/"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sciactive/tilmeld-client.git"
  },
  "keywords": [
    "users",
    "groups",
    "user management",
    "user manager",
    "group management",
    "group manager",
    "acl",
    "nymph"
  ],
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sciactive/tilmeld-client/issues"
  },
  "homepage": "https://github.com/sciactive/tilmeld-client#readme",
  "dependencies": {
    "nymph-client": "^4.0.0-beta.13",
    "pform": "^3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^5.3.0",
    "eslint-config-semistandard": "^12.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "svelte": "^2.9.10",
    "svelte-cli": "^3.0.0",
    "umailphp": "^2.4"
  }
}
